        android:textSize="48sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="352dp"
        tools:layout_editor_absoluteY="688dp" />

    <ImageView
        android:id="@+id/homeFlag"
        android:layout_width="887dp"
        android:layout_height="298dp"
        android:background="@drawable/background_bott"
        android:src="@drawable/stateless_rgb_color_icon"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="106dp"
        tools:layout_editor_absoluteY="858dp" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="80dp"
        android:layout_height="117dp"
        android:src="@drawable/images"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="817dp"
        tools:layout_editor_absoluteY="955dp" />

    <ImageView
        android:id="@+id/toristHome"
        android:layout_width="899dp"
